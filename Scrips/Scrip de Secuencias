-- Secuencias

  CREATE SEQUENCE  "SYSTEM"."CLAVE_ID"  MINVALUE 1 MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 220 CACHE 20 NOORDER  NOCYCLE ;

  CREATE SEQUENCE  "SYSTEM"."CT_ID"  MINVALUE 1 MAXVALUE 9999999999999 INCREMENT BY 1 START WITH 421 CACHE 20 NOORDER  NOCYCLE ;

  CREATE SEQUENCE  "SYSTEM"."TRABAJADOR_ID"  MINVALUE 1 MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 221 CACHE 20 NOORDER  NOCYCLE ;
  
